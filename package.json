{
  "name": "ijue-zenji",
  "version": "1.0.0",
  "description": "A place to learn about Zanzibar",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch ./src/_sass:src/_css",
    "build:sass": "sass ./src/_sass:src/_css",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:eleventy": "npx @11ty/eleventy",
    "dev": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy --serve",
    "debughard": "DEBUG=Dev:Eleventy* npx @11ty/eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmad1284/ijue-zenji.git"
  },
  "keywords": [
    "jamstack"
  ],
  "author": "ahmad1284",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahmad1284/ijue-zenji/issues"
  },
  "homepage": "https://github.com/ahmad1284/ijue-zenji#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.55.0"
  }
}
